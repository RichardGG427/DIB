ed_6","_hoisted_9","_Fragment","_renderList","_ctx","i","index","key","src","iconsrc","_toDisplayString","_component_van_tabs","color","_createBlock","_component_van_tab","title","tab","_component_Store","store_list","data","_component_Footer"],"sourceRoot":"","sources":["/Users/renxianggao/Desktop/DrivingInstructorBook/driving_instructor_book/src/pages/myHome/MyHome.vue"],"sourcesContent":["<template>\n  <div class=\"home\">\n    <div class=\"content\">\n      <!-- header -->\n      <div class=\"header\">\n        <div class=\"text\">Course</div>\n        <div class=\"location\">\n          <van-icon name=\"location\" />\n          <span>Melbourne Centre</span>\n          <van-icon name=\"arrow\" />\n        </div>\n      </div>\n      <!-- main content -->\n      <div class=\"main\">\n        <div class=\"main_bg\">\n          <div class=\"search\">\n            <input type=\"text\" />\n            <div class=\"search_text\">Search</div>\n          </div>\n          <div class=\"classify\">\n            <div class=\"big_classify\">\n              <div v-for=\"(i, index) in big_classify\" :key=\"index\">\n                <img :src=\"i.iconsrc\" class=\"icon\" />\n                {{ i.name }}\n              </div>\n            </div>\n            <div class=\"small_classify\">\n              <div v-for=\"(i, index) in small_classify\" :key=\"index\">\n                <img :src=\"i.iconsrc\" class=\"icon\" />\n                {{ i.name }}\n              </div>\n            </div>\n          </div>\n        </div>\n        <van-tabs class=\"van_tabs\" color=\"#ffc400\">\n          <van-tab v-for=\"(i, index) in content_nav_list\" :title=\"i.tab\">\n            <Store :store_list=\"i.data\" />\n          </van-tab>\n        </van-tabs>\n      </div>\n    </div>\n    <Footer />\n  </div>\n</template>\n<script>\nimport Footer from '../../components/Footer.vue';\nimport { reactive, toRefs } from 'vue';\nimport Store from './components/Store.vue';\n\n// import { makeRequiredProp } from 'vant/lib/utils';\nexport default {\n  components: {\n    Footer,\n    Store,\n  },\n  setup() {\n    let data = reactive({\n      big_classify: [\n        { name: 'Car', iconsrc: require('../../assets/car.png') },\n        { name: 'Truck', iconsrc: require('../../assets/truck.png') },\n        { name: 'Bus', iconsrc: require('../../assets/bus.png') },\n        { name: 'Motorcycle', iconsrc: require('../../assets/motorcycle.png') },\n      ],\n      small_classify: [\n        { name: 'Starting', iconsrc: require('../../assets/starting.png') },\n        { name: 'Parking', iconsrc: require('../../assets/parking.png') },\n        { name: 'Reversing', iconsrc: require('../../assets/reversing.png') },\n        { name: 'Shifting', iconsrc: require('../../assets/shifting.png') },\n        { name: 'Adjusting', iconsrc: require('../../assets/adjusting.png') },\n        { name: 'Signals', iconsrc: require('../../assets/signals.png') },\n        { name: 'Special', iconsrc: require('../../assets/special.png') },\n        { name: 'Overtaking', iconsrc: require('../../assets/overtaking.png') },\n        { name: 'Defensive', iconsrc: require('../../assets/defense.png') },\n        { name: 'All', iconsrc: require('../../assets/all.png') },\n      ],\n      content_nav_list: [\n        {\n          tab: 'Coupons',\n          data: [\n            {\n              pic: require('../../assets/overtaking.jpeg'),\n              title: 'Overtaking',\n              sales: '900',\n              price: '$290',\n              label: ['New', 'Hot'],\n            },\n            {\n              pic: require('../../assets/shifting.jpeg'),\n              title: 'Shifting',\n              sales: '888',\n              price: '$120',\n              label: ['New', 'Hot'],\n            },\n            {\n              pic: require('../../assets/reversing.webp'),\n              title: 'Reversing',\n              sales: '258',\n              price: '$100',\n              label: ['New'],\n            },\n            {\n              pic: require('../../assets/parking.jpeg'),\n              title: 'Parking',\n              sales: '555',\n              price: '$150',\n              label: ['New'],\n            },\n          ],\n        },\n        {\n          tab: 'Sales',\n          data: [\n            {\n              pic: require('../../assets/parking.jpeg'),\n              title: 'Parking',\n              sales: '198',\n              price: '$300',\n              label: ['New', 'Hot'],\n            },\n          ],\n        },\n        {\n          tab: 'Hot',\n          data: [\n            {\n              pic: require('../../assets/defensive.jpeg'),\n              title: 'Defensive Driving',\n              sales: '1111',\n              price: '$300',\n              label: ['New', 'Hot'],\n            },\n            {\n              pic: require('../../assets/night.jpeg'),\n              title: 'Driving at night',\n              sales: '888',\n              price: '$20',\n              label: ['New', 'Hot'],\n            },\n          ],\n        },\n        {\n          tab: 'VIP',\n          data: [\n            {\n              pic: require('../../assets/parking.jpeg'),\n              title: 'Parking',\n              sales: '198',\n              price: '$300',\n              label: ['New', 'Hot'],\n            },\n            {\n              pic: require('../../assets/shifting.jpeg'),\n              title: 'Shifting',\n              sales: '888',\n              price: '$120',\n              label: ['New', 'Hot'],\n            },\n          ],\n        },\n      ],\n    });\n    return {\n      ...toRefs(data),\n    };\n  },\n};\n</script>\n\n<style lang=\"less\" scoped>\n/deep/ .van-tabs__wrap {\n  border-radius: 5px;\n  width: 100%;\n}\n/deep/ .van-tab__text--ellipsis {\n  font-size: 7px;\n}\n\n.van_tabs {\n  width: 140px;\n  margin-left: -10px;\n}\n.home {\n  display: flex;\n  flex-flow: column;\n  height: 100%;\n  font-size: 6px;\n  .content {\n    flex: 1;\n    overflow-y: auto;\n\n    .main {\n      margin-top: -30px;\n      .van_tabs {\n        padding: 0 20px 10px;\n      }\n      .main_bg {\n        background-image: linear-gradient(#fff, #f5f5f5);\n        padding: 5px 10px 0 5px;\n        border-radius: 10px 10px 0 0;\n        position: relative;\n        .classify {\n          padding: 20px 0;\n          .small_classify {\n            display: flex;\n            flex-wrap: wrap;\n            margin-top: 10px;\n            div {\n              display: flex;\n              justify-content: center;\n              flex-flow: column;\n              align-items: center;\n              width: 20%;\n            }\n\n            .icon {\n              width: 15px;\n              height: 15px;\n              margin: 5px;\n            }\n          }\n          .big_classify {\n            display: flex;\n            div {\n              flex: 1;\n              display: flex;\n              justify-content: center;\n              flex-flow: column;\n              align-items: center;\n              .icon {\n                width: 20px;\n                height: 20px;\n                margin-bottom: 5px;\n              }\n            }\n          }\n        }\n        .search {\n          position: relative;\n          .search_text {\n            position: absolute;\n            background-color: #ffc400;\n            right: -6px;\n            top: 0px;\n            width: 50px;\n            height: 17px;\n            border-radius: 10px;\n            text-align: center;\n            line-height: 16px;\n            font-size: 10px;\n          }\n          input {\n            width: 100%;\n            border: 1px solid #ffc400;\n            border-radius: 10px;\n            height: 15px;\n          }\n        }\n      }\n    }\n    .header {\n      background-image: linear-gradient(#ffc400, #fff);\n      display: flex;\n      justify-content: space-between;\n      align-items: center;\n      padding: 10px 10px 40px 10px;\n      .text {\n        font-size: 10px;\n        font-weight: 600;\n      }\n      .location {\n        span {\n          margin: 0 5px;\n        }\n        font-size: 7px;\n      }\n    }\n  }\n}\n</style>\n"]}]}