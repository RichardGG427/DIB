ourcesContent":["<template>\n  <div class=\"me\">\n    <Header title=\"Mine\" />\n    <div class=\"content\">\n      <div class=\"user-info\">\n        <div class=\"info\">\n          <img src=\"../../assets/mine.png\" />\n          <div class=\"user-desc\">\n            <span>Name：{{ name }}</span>\n            <span class=\"name\">What's Up：{{ describe }}</span>\n          </div>\n        </div>\n      </div>\n      <ul class=\"user-list\">\n        <li class=\"van-hairline--bottom\" @click=\"togo('./order')\">\n          <span>My Order</span>\n          <van-icon name=\"arrow\" />\n        </li>\n        <li class=\"van-hairline--bottom\" @click=\"togo('./address')\">\n          <span>Address Manage</span>\n          <van-icon name=\"arrow\" />\n        </li>\n        <li class=\"van-hairline--bottom\" @click=\"togo('./userinfoedit')\">\n          <span>Account Manage</span>\n          <van-icon name=\"arrow\" />\n        </li>\n      </ul>\n    </div>\n    <Footer />\n  </div>\n</template>\n\n<script>\nimport { reactive, toRefs } from 'vue';\nimport Footer from '../../components/Footer.vue';\nimport Header from '../../components/Header.vue';\nimport { useRouter } from 'vue-router';\nexport default {\n  components: {\n    Footer,\n    Header,\n  },\n  setup() {\n    const router = useRouter();\n    let userInfo = reactive({\n      name: 'Richard',\n      describe: 'Seek Light',\n    });\n\n    //路由跳转\n    const togo = (path) => {\n      router.push(path);\n    };\n    return {\n      togo,\n      ...toRefs(userInfo),\n    };\n  },\n};\n</script>\n\n<style lang=\"less\" scoped>\n.me {\n  height: 100%;\n  display: flex;\n  flex-flow: column;\n  .content {\n    flex: 1;\n    overflow-y: auto;\n    .user-info {\n      width: 94%;\n      margin: 5px;\n      height: 80px;\n      background: linear-gradient(90deg, #ffc400, #c7c351);\n      box-shadow: 0 2px 5px #ffc400;\n      border-radius: 6px;\n      .info {\n        position: relative;\n        display: flex;\n        width: 100%;\n        height: 100%;\n        padding: 15px 10px;\n        box-sizing: border-box;\n        img {\n          border-radius: 50%;\n          margin-top: 4px;\n          width: 30px;\n          height: 35px;\n        }\n        .user-desc {\n          display: flex;\n          flex-direction: column;\n          margin-left: 10px;\n          line-height: 20px;\n          font-size: 5px;\n          color: #fff;\n          justify-content: space-between;\n          span {\n            color: #fff;\n            font-size: 8px;\n            padding: 2px 0;\n          }\n        }\n        .account-setting {\n          position: absolute;\n          top: 50px;\n          right: 10px;\n          font-size: 7px;\n          color: #fff;\n          .van-icon-setting-o {\n            font-size: 8px;\n            vertical-align: -3px;\n            margin-right: 4px;\n          }\n        }\n      }\n    }\n    .user-list {\n      padding: 0 10px;\n      margin-top: 20px;\n      background-color: #fff;\n      li {\n        height: 20px;\n        line-height: 20px;\n        display: flex;\n        justify-content: space-between;\n        font-size: 7px;\n        .van-icon-arrow {\n          margin-top: 6px;\n        }\n      }\n    }\n  }\n}\n</style>\n"]}]}