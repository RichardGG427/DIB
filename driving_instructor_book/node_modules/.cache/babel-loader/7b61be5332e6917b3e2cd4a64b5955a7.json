/src/pages/myHome/components/StoreItem.vue"],"sourcesContent":["<template>\n  <div class=\"item\" @click=\"toStore(item)\">\n    <img :src=\"item.pic\" alt=\"Course\" />\n    <div class=\"item_right\">\n      <div class=\"title\">{{ item.title }}</div>\n      <div class=\"sales\">Sales: {{ item.sales }}</div>\n      <div class=\"price\">From {{ item.price }} pick-up free</div>\n      <div class=\"label\">\n        <div v-for=\"(i, index) in item.label\">{{ i }}</div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { useRouter } from 'vue-router';\n// import router from '../../../router';\n\nexport default {\n  props: ['item'],\n  setup() {\n    const router = useRouter();\n    const toStore = (item) => {\n      router.push({ path: './store', query: { title: item.title } });\n    };\n    return {\n      toStore,\n    };\n  },\n};\n</script>\n<style lang=\"less\" scoped>\n.item:not(:first-child) {\n  margin-top: 5px;\n}\n.item {\n  background-color: #fff;\n  border-radius: 5px;\n  display: flex;\n  padding: 5px;\n\n  img {\n    width: 45px;\n    height: 45px;\n    border-radius: 5px;\n    margin-right: 5px;\n  }\n\n  .item_right {\n    display: flex;\n    flex-flow: column;\n    justify-content: space-between;\n    .title {\n      font-size: 7px;\n    }\n    .label {\n      display: flex;\n      div {\n        background-color: #ffc40047;\n        padding: 2px 5px;\n        border-radius: 3px;\n      }\n      div:not(:first-child) {\n        margin-left: 5px;\n      }\n    }\n  }\n}\n</style>\n"]}]}