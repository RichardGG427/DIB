 0,\n      items: [],\n      subItem: [],\n    });\n\n    // 数据的初始化\n    const init = () => {\n      let newList = [];\n      props.foodData?.items?.map((i, index) => {\n        newList.push({ text: i.text });\n        if (data.active === index) {\n          data.subItem = i.children;\n        }\n      });\n      data.items = newList;\n    };\n    init();\n\n    // 点击左侧的导航\n    const navClick = (i) => {\n      data.active = i;\n      init();\n    };\n\n    // 切换步进器\n    const addClick = (i) => {\n      data.subItem.forEach((item) => {\n        if (item.id === i) {\n          item.add = false;\n          item.num += 1;\n        }\n      });\n    };\n\n    // 步进器增加触发事件\n    const onChange = (value, detail) => {\n      data.subItem.forEach((item) => {\n        if (item.id === detail.name) {\n          item.num = value;\n        }\n      });\n    };\n\n    return {\n      ...toRefs(data),\n      navClick,\n      addClick,\n      onChange,\n    };\n  },\n};\n</script>\n\n<style lang='less' scoped>\n.food_list {\n  margin-top: 20px;\n  .item_bg {\n    padding: 10px;\n  }\n}\n\n/deep/ .van-tree-select__item--active {\n  color: #ffc400;\n}\n/deep/ .van-sidebar-item--select::before {\n  background-color: #ffc400;\n}\n</style>"]}]}