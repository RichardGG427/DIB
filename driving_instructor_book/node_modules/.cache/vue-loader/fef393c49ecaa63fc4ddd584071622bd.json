CAAC,CAAC,CAAC,EAAE;EAC7B,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC;AAChB;AACA,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE;EACpC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;AAChB;AACA,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE;EACvC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;AAC3B","file":"/Users/renxianggao/Desktop/DrivingInstructorBook/driving_instructor_book/src/pages/myStore/components/CourseList.vue","sourceRoot":"","sourcesContent":["<template>\n  <div class=\"course_list\" v-if=\"index === 0\">\n    <van-tree-select\n      height=\"88vw\"\n      :items=\"items\"\n      :main-active-index.sync=\"active\"\n      @click-nav=\"navClick\"\n    >\n      <template #content>\n        <div v-for=\"(i, index) in subItem\" class=\"item_bg\">\n          <CourseAdd\n            :item=\"i\"\n            :showAdd=\"true\"\n            :addClick=\"addClick\"\n            :onChange=\"onChange\"\n          />\n        </div>\n      </template>\n    </van-tree-select>\n  </div>\n  <div v-else>{{ courseData.content }}</div>\n</template>\n\n<script>\nimport { reactive, toRefs } from 'vue';\nimport CourseAdd from '../../../components/CourseAdd.vue';\n\nexport default {\n  props: ['index', 'courseData'],\n  components: {\n    CourseAdd,\n  },\n  setup(props) {\n    let data = reactive({\n      active: 0,\n      items: [],\n      subItem: [],\n    });\n\n    //data initialization\n    const init = () => {\n      let newList = [];\n      props.courseData?.items?.map((i, index) => {\n        newList.push({ text: i.text });\n        if (data.active === index) {\n          data.subItem = i.children;\n        }\n      });\n      data.items = newList;\n    };\n    init();\n\n    //click left nav menu\n    const navClick = (i) => {\n      data.active = i;\n      init();\n    };\n\n    //switch stepper\n    const addClick = (i) => {\n      data.subItem.forEach((item) => {\n        if (item.id === i) {\n          item.add = false;\n          item.num += 1;\n        }\n      });\n    };\n\n    //stepper add active event\n    const onChange = (value, detail) => {\n      data.subItem.forEach((item) => {\n        if (item.id === detail.name) {\n          item.num = value;\n        }\n      });\n    };\n\n    return {\n      ...toRefs(data),\n      navClick,\n      addClick,\n      onChange,\n    };\n  },\n};\n</script>\n<style lang=\"less\" scoped>\n.course_list {\n  margin-top: 20px;\n  .item_bg {\n    padding: 10px;\n  }\n}\n\n/deep/ .van-sidebar-item__text {\n  font-size: 7px;\n}\n/deep/ .van-tree-select__item--active {\n  color: #ffc400;\n}\n/deep/ .van-sidebar-item--select::before {\n  background-color: #ffc400;\n}\n</style>\n"]}]}